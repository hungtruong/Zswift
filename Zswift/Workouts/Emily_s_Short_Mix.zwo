            <textevent timeoffset="15" message="High cadence. "/>
            <textevent timeoffset="45" message="Go go go! "/>
            <textevent timeoffset="75" message="Don&apos;t drop your effort. "/>
            <textevent timeoffset="90" message="Halfway. "/>
            <textevent timeoffset="110" message="Final minute ahead! "/>
            <textevent timeoffset="120" message="Anything and everything you&apos;ve got. "/>
