<workout_file>
    <author>E.Mullen</author>
    <name>Emily's Short Mix</name>
    <description>You've got 30min to spare and you wanna make it count.  </description>
    <sportType>bike</sportType>
    <tags/>
    <workout>
        <Warmup Duration="180.00002" PowerLow="0.44999996" PowerHigh="0.85000002" pace="2">
            <textevent timeoffset="0" message="Welcome to Emily&apos;s Short Mix!"/>
            <textevent timeoffset="10" message="This workout was designed to give you a great workout in 30min. "/>
            <textevent timeoffset="20" message="Turn up the volume on your favorite tunes and get ready for fun."/>
            <textevent timeoffset="30" message="You will sweat in this effort. But it&apos;s a short workout.  "/>
