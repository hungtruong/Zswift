            <textevent timeoffset="15" message="Keep your cadence high. "/>
            <textevent timeoffset="30" message="30 seconds.  Keep it up. "/>
            <textevent timeoffset="50" message="10 seconds!  Push thru! "/>
        </SteadyState>
        <SteadyState Duration="90.000008" Power="0.75" pace="2">
