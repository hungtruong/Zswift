            <textevent timeoffset="60" message="Making every minute count. "/>
            <textevent timeoffset="120" message="Keep your cadence up.  Don&apos;t let it slow! "/>
            <textevent timeoffset="160" message="Final 3 minutes of work ahead. "/>
            <textevent timeoffset="170" message="YES. YOU. CAN.   "/>
            <textevent timeoffset="175" message="Let&apos;s finish this out. "/>
        </SteadyState>
